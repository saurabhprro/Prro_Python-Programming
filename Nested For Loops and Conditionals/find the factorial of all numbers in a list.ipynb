   "source": [
    "Given a list of positive integers, find the factorial of all the numbers."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 2, 6, 720, 24, 720, 5040]\n"
     ]
    }
   ],
   "source": [
    "a=[1,2,3,6,4,6,7]\n",
    "fact_list=[]\n",
    "p=1\n",
    "for i in a:\n",
    "    p=1\n",
    "    for j in range(2,i+1):\n",
    "        p=j*p\n",
    "    fact_list.append(p)\n",
    "\n",
    "\n",
    "       \n",
    "        \n",
    "print(fact_list)"
